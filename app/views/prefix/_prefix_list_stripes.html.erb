<tr class="<%= prefix_list_stripes_counter.modulo(2).nonzero? ? "dk_gray" : "lt_gray" %>">
  <td align="center">
    <% rid = prefix_list_stripes.id %>
    <%= prefix_list_stripes.pcb_mnemonic %> 
  </td>
  <td align="center">
    <%= prefix_list_stripes.loaded_prefix %>
  </td>
  <td align="center">
    <%= prefix_list_stripes.unloaded_prefix %>
  </td>
  <td align="center">
    <% if prefix_list_stripes.active? %>
      Yes
    <% else %>
      No
    <% end %>
  </td>
</tr>
