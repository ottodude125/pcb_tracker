<% @heading = "PCB Engineering - #{@board_design_entry.pcb_number} Design Entry - Add #{@document_type.name}"%><% form_tag({:action        => 'save_document',              :id            => @board_design_entry.id,             :user_action   => @user_action,             :viewer        => params[:viewer],             :document_type => @document_type.name}, :multipart => true ) do %><hr class="div" />  <table border="0"         width="90%"         align="center">    <tr>      <th align="right"><%= @document_type.name %> Filename:</th>      <td><%= file_field('document', 'document') %></td>    </tr>      </table><hr class="div" />  <input type="submit" value="Upload File" />  <input type="button" value="Cancel" onClick="parent.location='<%= url_for(:action      => 'view_attachments',                                                                             :id          => @board_design_entry.id,                                                                            :user_action => params[:user_action]) %>'" /><% end %><%# debug params %>